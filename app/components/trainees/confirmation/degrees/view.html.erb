<% if trainee.degrees.present? %>
  <% trainee.degrees.each do |degree| %>
    <%= render_component SummaryCard::View.new(title: degree_title(degree),
        heading_level: 2,
        rows: get_degree_rows(degree)
      )
    %>
  <% end %>
<% end %>

<div class="govuk-form-group">
  <%= govuk_link_to("Add another degree",
                      new_trainee_degree_path(trainee),
                      class:"govuk-button govuk-button--secondary") %>
</div>
